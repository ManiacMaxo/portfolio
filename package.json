{
  "name": "portfolio",
  "author": "Victor Gorchilov <victor@gorchilov.com>",
  "version": "0.2.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "create:component": "node ./helper_scripts/commands/create_component.js",
    "create:page": "node ./helper_scripts/commands/create_page.js",
    "dev": "next",
    "format": "prettier --write .",
    "lint": "eslint . --ext ts --ext tsx --ext js",
    "start": "next start",
    "type-check": "tsc --pretty --noEmit"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.@(ts|tsx)": [
      "yarn lint",
      "yarn format"
    ]
  },
  "dependencies": {
    "@chakra-ui/react": "^1.5.0",
    "@emotion/react": "^11.1.5",
    "@emotion/styled": "^11.3.0",
    "framer-motion": "^4.1.3",
    "next": "^10.2.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-icons": "^4.2.0"
  },
  "devDependencies": {
    "@types/node": "^15.0.2",
    "@types/react": "^17.0.5",
    "@types/react-dom": "^17.0.4",
    "@typescript-eslint/eslint-plugin": "^4.23.0",
    "@typescript-eslint/parser": "^4.23.0",
    "dotenv": "^9.0.2",
    "eslint": "^7.26.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-react": "^7.23.2",
    "handlebars": "^4.7.7",
    "husky": "^6.0.0",
    "inquirer": "^8.0.0",
    "lint-staged": "^11.0.0",
    "prettier": "^2.3.0",
    "sass": "^1.32.12",
    "typescript": "^4.2.4"
  }
}
